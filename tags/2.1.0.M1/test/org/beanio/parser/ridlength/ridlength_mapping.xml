<?xml version='1.0' encoding='UTF-8' ?>
<beanio xmlns="http://www.beanio.org/2012/03" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2012/03 http://www.beanio.org/2012/03/mapping.xsd">

  <stream name="r1" format="csv">
    <record name="afew" class="map" ridLength="3+">
      <field name="values" collection="list" type="int" occurs="3+" />
    </record>
    <record name="acouple" class="map" ridLength="0-2">
      <field name="values" collection="list" type="int" occurs="0-2" />
    </record>
  </stream>
  
</beanio>